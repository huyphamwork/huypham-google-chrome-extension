{
  "manifest_version": 3,
  "name": "Huy Pham Google Chrome Extension",
  "description": "This is example google chrome extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "background": {
    "service_worker": "scripts/service-worker.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation"
  ]
}